{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon server.js",
    "gateway:dev": "NODE_ENV=development nodemon gateway-server.js",
    "gateway:start": "NODE_ENV=production node gateway-server.js",
    "start": "NODE_ENV=production node server.js",
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "npm run test -- --watch",
    "db:schema": "psql -U postgres -f models/postgresql/db/schema.sql",
    "db:seed": "psql -U postgres -f models/postgresql/db/seed.sql",
    "db:reset": "psql -U postgres -f models/postgresql/db/schema.sql && psql -U postgres -f models/postgresql/db/seed.sql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.0",
    "bcrypt": "5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "9.0.2",
    "mongodb": "^6.13.0",
    "mongoose": "^8.10.0",
    "morgan": "1.10.0",
    "nodemailer": "^6.10.0",
    "pg-promise": "^11.10.2",
    "socket.io": "4.8.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.17.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "jest": "^29.7.0",
    "supertest": "^7.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "directories": {
    "test": "tests"
  },
  "description": ""
}
