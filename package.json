{
  "name": "ai-journal",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start:client": "npm --prefix client start",
    "start:server": "npm --prefix server start",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "install:client": "npm --prefix client install",
    "install:server": "npm --prefix server install",
    "install:all": "npm install",
    "db:schema": "node server/run-schema.js",
    "db:seed": "node server/run-seed.js",
    "db:reset": "npm run db:schema && npm run db:seed"
  },
  "keywords": [],
  "author": "Yanako Lajara",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
